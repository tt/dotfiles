[alias]
	aa = add --all
	ai = add --interactive
	br = branch
	ca = commit --amend
	ci = commit
	co = checkout
	dc = diff --cached
	di = diff
	ff = merge --ff-only
	lg = log --graph
	st = status
[branch]
        sort = -committerdate
[commit]
	verbose = true
[fetch]
	prune = true
[gpg]
	format = ssh
[gpg "ssh"]
	defaultKeyCommand = ssh-add -L
